, \"DF 2\") {}\n\n    fallback() external payable {}\n\n    receive() external payable {}\n\n    function couponMint(bytes32[] calldata proof, uint256 amount)\n        external\n        nonReentrant\n    {\n        uint256 tokenId = totalSupply() + 1;\n        require(saleIsActive, \"Sale is not active\");\n        require(msg.sender == tx.origin, \"Interaction not allowed\");\n        require(\n            couponMintStatus[msg.sender] != true,\n            \"You can't mint over one nft using same coupon.\"\n        );\n        require(tokenId + amount - 1 <= maxcouponTokens, \"Mint/order exceeds coupon supply\");\n        bytes32 leaf = keccak256(abi.encodePacked(msg.sender, amount));\n        require(\n            MerkleProof.verify(proof, couponRoot, leaf),\n            \"This user is not allowed to mint\"\n        );\n        for (uint256 i = 0; i < amount; i++) {\n            _safeMint(msg.sender, (tokenId + i - 1));\n        }\n\n        if (couponUsers.contains(msg.sender) == false) {\n            couponUsers.add(msg.sender);\n        }\n        couponMintStatus[msg.sender] = true;\n        emit TotalMinted(msg.sender, totalSupply());\n    }\n\n    function publicMint() external nonReentrant {\n        uint256 tokenId = totalSupply() + 1;\n        require(saleIsActive, \"Sale is not active\");\n        require(tokenId > maxcouponTokens, \"You can't public mint yet\");\n        require(tokenId <= collectionTotal, \"Mint/order exceeds total supply\");\n        require(publicMintLimit[msg.sender] < 9, \"You can't mint nfts over 8.\");\n\n        _safeMint(msg.sender, tokenId);\n        publicMintLimit[msg.sender]++;\n        emit TotalMinted(msg.sender, totalSupply());\n    }\n\n    function publicBatchMint(uint256 amount)\n        external\n        onlyOwner\n        nonReentrant\n    {\n        uint256 tokenId = totalSupply() + 1;\n        require(saleIsActive, \"Sale is not active\");\n        require(tokenId > maxcouponTokens, \"You can't public mint yet\");\n        require(tokenId + amount - 1 <= collectionTotal, \"Mint/order exceeds total supply\");\n\n        for (uint256 i = 0; i < amount; i++) {\n            _safeMint(msg.sender, (tokenId + i -1));\n            emit TotalMinted(msg.sender, totalSupply());\n        }\n    }\n\n    function tokenURI(uint256 tokenId)\n        external\n        view\n        virtual\n        override\n        returns (string memory uri)\n    {\n        require(\n            _exists(tokenId),\n            \"ERC721Metadata: URI query for nonexistent token\"\n        );\n\n        return string(abi.encodePacked(_tokneURI, toString(tokenId), \".json\"));\n    }\n\n    function setBaseURI(string calldata _newBaseURI) external onlyOwner {\n        _tokneURI = _newBaseURI;\n    }\n\n    function _generateLeaf(address account) internal pure returns (bytes32) {\n        return keccak256(abi.encodePacked(account));\n    }\n\n    function _generateCouponLeaf(string memory coupon)\n        internal\n        pure\n        returns (bytes32)\n    {\n        return keccak256(abi.encodePacked(coupon));\n    }\n\n    function toggleSale(bool _saleIsActive) external onlyOwner {\n        if (saleIsActive != _saleIsActive) {\n            saleIsActive = _saleIsActive;\n        }\n    }\n\n    function setRoot(string memory _root) external onlyOwner {\n        merkleRoot = _root;\n    }\n\n    function setCoupons(bytes32 _root) external onlyOwner {\n        couponRoot = _root;\n\n        for (uint256 i = 0; i < couponUsers.length(); i++) {\n            couponMintStatus[couponUsers.at(i)] = false;\n        }\n    }\n\n    function balanceOf(address owner)\n        public\n        view\n        virtual\n        override\n        returns (uint256)\n    {\n        require(\n            owner != address(0),\n            \"ERC721: balance query for the zero address\"\n        );\n        return _balances[owner].length;\n    }\n\n    function tokenOfOwnerByIndex(address owner, uint256 index)\n        public\n        view\n        virtual\n        override\n        returns (uint256 tokenId)\n    {\n        require(\n            index < ERC721B.balanceOf(owner),\n            \"ERC721Enumerable: owner index out of bounds\"\n        );\n        return _balances[owner][index];\n    }\n\n    function _beforeTokenTransfer(\n        address from,\n        address to,\n        uint256 tokenId\n    ) internal virtual override {\n        address zero = address(0);\n        if (from != zero || to == zero) {\n            uint256 length = _balances[from].length;\n            for (uint256 i; i < length; ++i) {\n                if (_balances[from][i] == tokenId) {\n                    _balances[from][i] = _balances[from][length - 1];\n                    _balances[from].pop();\n                    break;\n                }\n            }\n            delete length;\n        }\n\n        if (from == zero || to != zero) {\n            _balances[to].push(tokenId);\n        }\n    }\n\n    bytes16 private constant _HEX_SYMBOLS = \"0123456789abcdef\";\n\n    /**\n     * @dev Converts a `uint256` to its ASCII `string` decimal representation.\n     */\n    function toString(uint256 value) internal pure returns (string memory) {\n        if (value == 0) {\n            return \"0\";\n        }\n        uint256 temp = value;\n        uint256 digits;\n        while (temp != 0) {\n            digits++;\n            temp /= 10;\n        }\n        bytes memory buffer = new bytes(digits);\n        while (value != 0) {\n            digits -= 1;\n            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));\n            value /= 10;\n        }\n        return string(buffer);\n    }\n\n    /**\n     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation.\n     */\n    function toHexString(uint256 value) internal pure returns (string memory) {\n        if (value == 0) {\n            return \"0x00\";\n        }\n        uint256 temp = value;\n        uint256 length = 0;\n        while (temp != 0) {\n            length++;\n            temp >>= 8;\n        }\n        return toHexString(value, length);\n    }\n\n    /**\n     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation with fixed length.\n     */\n    function toHexString(uint256 value, uint256 length)\n        internal\n        pure\n        returns (string memory)\n    {\n        bytes memory buffer = new bytes(2 * length + 2);\n        buffer[0] = \"0\";\n        buffer[1] = \"x\";\n        for (uint256 i = 2 * length + 1; i > 1; --i) {\n            buffer[i] = _HEX_SYMBOLS[value & 0xf];\n            value >>= 4;\n        }\n        require(value == 0, \"Strings: hex length insufficient\");\n        return string(buffer);\n    }\n\n    function withdraw(address wallet) public onlyOwner {\n        uint256 balance = address(this).balance;\n        require(balance > 0, \"Insufficent balance\");\n\n        _widthdraw(wallet, address(this).balance);\n    }\n\n    function withdrawAll() public onlyOwner {\n        uint256 balance = address(this).balance;\n        require(balance > 0, \"Insufficent balance\");\n        _widthdraw(w1, ((balance * 10) / 100));\n        _widthdraw(w2, ((balance * 20) / 100));\n        _widthdraw(w3, ((balance * 35) / 100));\n        _widthdraw(w4, ((balance * 35) / 100));\n    }\n\n    function _widthdraw(address _address, uint256 _amount) private {\n        (bool success, ) = _address.call{value: _amount}(\"\");\n        require(success, \"Failed to withdraw Ether\");\n    }\n\n    //****Functions below added by HalfSuperShop.com dev  ****\n    function setAllowedMinter (address _user ,uint256 _amount) external onlyOwner{\n        if(_amount == 0){\n            allowMint[_user] = false;\n        }\n        else{\n            allowMint[_user] = true;\n        }\n        allowMintAmount[_user] = _amount;\n    }\n\n    function allowedMint (uint256 _amount) external nonReentrant{\n        uint256 tokenId = totalSupply() + 1;\n        require(_amount != 0, \"Amount must be 1 or more\");\n        require(tokenId + _amount - 1 <= collectionTotal, \"Mint/order exceeds total supply\");\n        require(saleIsActive, \"Sale is not active\");\n        require(allowMint[msg.sender], \"You are not allowed to mint\");\n        require(_amount <= allowMintAmount[msg.sender], \"You can't mint more than allowed\");\n\n        if(_amount > 1){\n            for (uint256 i = 0; i < _amount; i++) {\n                _safeMint(msg.sender, (tokenId + i -1));\n            }\n        }\n        else{\n            _safeMint(msg.sender, tokenId);\n        }\n\n        allowMintAmount[msg.sender] -= _amount;\n\n        if(allowMintAmount[msg.sender] <= 0){\n            allowMint[msg.sender] = false;\n        }\n        emit TotalMinted(msg.sender, totalSupply());\n    }\n\n    function setMaxCoupons (uint256 _amount) external onlyOwner{\n        maxcouponTokens = _amount;\n    }\n}"
