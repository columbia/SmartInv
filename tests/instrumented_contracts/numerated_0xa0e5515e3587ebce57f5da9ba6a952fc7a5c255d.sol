1391     function swapBack() private {
1392         uint256 contractBalance = balanceOf(address(this));
1393         uint256 totalTokensToSwap = tokensForLiquidity + tokensForMarketing + tokensForDev;
1394         bool success;
1395  
1396         if(contractBalance == 0 || totalTokensToSwap == 0) {return;}
1397  
1398         if(contractBalance > swapTokensAtAmount * 20){
1399           contractBalance = swapTokensAtAmount * 20;
1400         }
1401  
1402         // Halve the amount of liquidity tokens
1403         uint256 liquidityTokens = contractBalance * tokensForLiquidity / totalTokensToSwap / 2;
1404         uint256 amountToSwapForETH = contractBalance.sub(liquidityTokens);
1405  
1406         uint256 initialETHBalance = address(this).balance;
1407  
1408         swapTokensForEth(amountToSwapForETH); 
1409  
1410         uint256 ethBalance = address(this).balance.sub(initialETHBalance);
1411  
1412         uint256 ethForMarketing = ethBalance.mul(tokensForMarketing).div(totalTokensToSwap);
1413         uint256 ethForDev = ethBalance.mul(tokensForDev).div(totalTokensToSwap);
1414         uint256 ethForLiquidity = ethBalance - ethForMarketing - ethForDev;
1415  
1416  
1417         tokensForLiquidity = 0;
1418         tokensForMarketing = 0;
1419         tokensForDev = 0;
1420  
1421         (success,) = address(devWallet).call{value: ethForDev}("");
1422  
1423         if(liquidityTokens > 0 && ethForLiquidity > 0){
1424             addLiquidity(liquidityTokens, ethForLiquidity);
1425             emit SwapAndLiquify(amountToSwapForETH, ethForLiquidity, tokensForLiquidity);
1426         }
1427  
1428         (success,) = address(marketingWallet).call{value: address(this).balance}("");
1429     }
