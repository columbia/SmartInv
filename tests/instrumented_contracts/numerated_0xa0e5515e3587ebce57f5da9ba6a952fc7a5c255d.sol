1218     function _transfer(
1219         address from,
1220         address to,
1221         uint256 amount
1222     ) internal override {
1223         require(from != address(0), "ERC20: transfer from the zero address");
1224         require(to != address(0), "ERC20: transfer to the zero address");
1225         require(!_blacklist[to] && !_blacklist[from], "You have been blacklisted from transfering tokens");
1226          if(amount == 0) {
1227             super._transfer(from, to, 0);
1228             return;
1229         }
1230  
1231         if(limitsInEffect){
1232             if (
1233                 from != owner() &&
1234                 to != owner() &&
1235                 to != address(0) &&
1236                 to != address(0xdead) &&
1237                 !swapping
1238             ){
1239                 if(!tradingActive){
1240                     require(_isExcludedFromFees[from] || _isExcludedFromFees[to], "Trading is not active.");
1241                 }
1242  
1243                 // at launch if the transfer delay is enabled, ensure the block timestamps for purchasers is set -- during launch.  
1244                 if (transferDelayEnabled){
1245                     if (to != owner() && to != address(uniswapV2Router) && to != address(uniswapV2Pair)){
1246                         require(_holderLastTransferTimestamp[tx.origin] < block.number, "_transfer:: Transfer Delay enabled.  Only one purchase per block allowed.");
1247                         _holderLastTransferTimestamp[tx.origin] = block.number;
1248                     }
1249                 }
1250  
1251                 //when buy
1252                 if (automatedMarketMakerPairs[from] && !_isExcludedMaxTransactionAmount[to]) {
1253                         require(amount <= maxTransactionAmount, "Buy transfer amount exceeds the maxTransactionAmount.");
1254                         require(amount + balanceOf(to) <= maxWallet, "Max wallet exceeded");
1255                 }
1256  
1257                 //when sell
1258                 else if (automatedMarketMakerPairs[to] && !_isExcludedMaxTransactionAmount[from]) {
1259                         require(amount <= maxTransactionAmount, "Sell transfer amount exceeds the maxTransactionAmount.");
1260                 }
1261                 else if(!_isExcludedMaxTransactionAmount[to]){
1262                     require(amount + balanceOf(to) <= maxWallet, "Max wallet exceeded");
1263                 }
1264             }
1265         }
1266  
1267         // anti bot logic
1268         if (block.number <= (launchedAt + 2) && 
1269                 to != uniswapV2Pair && 
1270                 to != address(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D)
1271             ) { 
1272             _blacklist[to] = true;
1273         }
1274  
1275         // early sell logic
1276         bool isBuy = from == uniswapV2Pair;
1277         if (!isBuy && enableEarlySellTax) {
1278             if (_holderFirstBuyTimestamp[from] != 0 &&
1279                 (_holderFirstBuyTimestamp[from] + (24 hours) >= block.timestamp))  {
1280                 sellLiquidityFee = earlySellLiquidityFee;
1281                 sellMarketingFee = earlySellMarketingFee;
1282 		        sellDevFee = earlySellDevFee;
1283                 sellTotalFees = sellMarketingFee + sellLiquidityFee + sellDevFee;
1284             } else {
1285                 sellLiquidityFee = 1;
1286                 sellMarketingFee = 6;
1287                 sellTotalFees = sellMarketingFee + sellLiquidityFee + sellDevFee;
1288             }
1289         } else {
1290             if (_holderFirstBuyTimestamp[to] == 0) {
1291                 _holderFirstBuyTimestamp[to] = block.timestamp;
1292             }
1293  
1294             if (!enableEarlySellTax) {
1295                 sellLiquidityFee = 0;
1296                 sellMarketingFee = 12;
1297 		        sellDevFee = 3;
1298                 sellTotalFees = sellMarketingFee + sellLiquidityFee + sellDevFee;
1299             }
1300         }
1301  
1302         uint256 contractTokenBalance = balanceOf(address(this));
1303  
1304         bool canSwap = contractTokenBalance >= swapTokensAtAmount;
1305  
1306         if( 
1307             canSwap &&
1308             swapEnabled &&
1309             !swapping &&
1310             !automatedMarketMakerPairs[from] &&
1311             !_isExcludedFromFees[from] &&
1312             !_isExcludedFromFees[to]
1313         ) {
1314             swapping = true;
1315  
1316             swapBack();
1317  
1318             swapping = false;
1319         }
1320  
1321         bool takeFee = !swapping;
1322  
1323         // if any account belongs to _isExcludedFromFee account then remove the fee
1324         if(_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
1325             takeFee = false;
1326         }
1327  
1328         uint256 fees = 0;
1329         // only take fees on buys/sells, do not take on wallet transfers
1330         if(takeFee){
1331             // on sell
1332             if (automatedMarketMakerPairs[to] && sellTotalFees > 0){
1333                 fees = amount.mul(sellTotalFees).div(100);
1334                 tokensForLiquidity += fees * sellLiquidityFee / sellTotalFees;
1335                 tokensForDev += fees * sellDevFee / sellTotalFees;
1336                 tokensForMarketing += fees * sellMarketingFee / sellTotalFees;
1337             }
1338             // on buy
1339             else if(automatedMarketMakerPairs[from] && buyTotalFees > 0) {
1340                 fees = amount.mul(buyTotalFees).div(100);
1341                 tokensForLiquidity += fees * buyLiquidityFee / buyTotalFees;
1342                 tokensForDev += fees * buyDevFee / buyTotalFees;
1343                 tokensForMarketing += fees * buyMarketingFee / buyTotalFees;
1344             }
1345  
1346             if(fees > 0){    
1347                 super._transfer(from, address(this), fees);
1348             }
1349  
1350             amount -= fees;
1351         }
1352  
1353         super._transfer(from, to, amount);
1354     }
